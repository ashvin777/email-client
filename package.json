    "lokijs": "^1.5.5",
    "mailparser": "^2.2.0",
    "nedb": "^1.8.0",
    "node-sass": "^4.9.2",
    "request": "^2.87.0",
    "sass-loader": "^7.0.2",
    "vue": "^2.5.16",
    "vue-infinite-loading": "^2.3.1",
    "vue-infinite-scroll": "^2.0.2",
    "vue-moment": "^4.0.0",
